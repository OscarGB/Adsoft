package clase20170215;

public class Disco {
	private String name;
	private String group;
	private int year;
	private int numCanciones;
	
	public Disco(String name, String group, int year, int numCanciones){
		this.name = name;
		this.group = group;
		this.year = year;
		this.numCanciones = numCanciones;
	}
}
